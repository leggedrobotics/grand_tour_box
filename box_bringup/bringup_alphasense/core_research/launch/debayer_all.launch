<launch>

<include file="$(find bringup_alphasense)/launch/debayer_single_camera.launch">
    <arg name="camera_name" value="cam0"/>
    <arg name="input_topic" value="alphasense_driver_node/cam0"/>
    <arg name="output_prefix" value="alphasense_driver_node/cam0"/>
    <arg name="output_frame" value="cam0_sensor_frame_helper"/>
    <arg name="debayer/enabled" value="false"/>
    <arg name="white_balance/enabled" value="true"/>
    <arg name="undistortion/calibration_file" value="$(find core_calibration)/calibration/alphasense/cam0.yaml"/>
</include>

