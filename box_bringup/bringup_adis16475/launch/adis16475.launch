<launch>
  <arg name="box_namespace" default="gt_box"/>
  <arg name="calibration" default="no"/>
  <group if="$(eval arg('calibration') == 'no' or arg('calibration') == 'imu')" ns="$(arg box_namespace)">

    <node pkg="adis16475_node" type="imu_publisher" name="adis16475_node" output="screen">
        <param name="decimation_factor" value="5">
        <param name="fir_filter" value="4"/>
        <param name="burst" value="false"/>
    </node>

  </group>
</launch>