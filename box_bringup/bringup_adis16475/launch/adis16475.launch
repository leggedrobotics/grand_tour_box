<launch>
  <arg name="box_namespace" default="gt_box"/>

    <!-- Arguments -->
    <arg name="variant"                 default="2"/>
    <arg name="sampling_frequency"      default="2000"/>
    <arg name="uri"                     default="local:"/>
    <arg name="context_timeout_ms"      default="5000"/>

    <env name="ROSCONSOLE_FORMAT" value="[${node}] [${function}:${line}]: ${message}"/>
    
    <group ns="$(arg box_namespace)">
      <node pkg="adi_imu_driver_ros" type="adis16475_node" name="adis16475_node" output="screen" launch-prefix="">
          <!-- Arguments -->
          <param name="capability_group"      value="Driver"/>
          <param name="variant"               value="$(arg variant)"/>
          <param name="sampling_frequency"    value="$(arg sampling_frequency)"/>
          <param name="uri"                   value="$(arg uri)"/>
          <param name="context_timeout_ms"    value="$(arg context_timeout_ms)"/>
      </node>
    </group>
</launch>
