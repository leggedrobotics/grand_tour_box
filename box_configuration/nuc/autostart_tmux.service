[Unit]
Description=Autostart Terminal and Run tmuxp
After=network-online.target
After=phc2sys_system.service
After=ptp4l.service

[Service]
Type=forking
ExecStartPre=/bin/sleep 30
ExecStart=/usr/bin/tmuxp load /home/rsl/catkin_ws/src/grand_tour_box/box_bringup/box_launch/tmux/box_nuc.yaml -d
User=rsl

[Install]
WantedBy=default.target
