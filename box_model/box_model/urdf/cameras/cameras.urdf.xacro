<?xml version="1.0" ?>

<robot name="cameras" xmlns:xacro="http://www.ros.org/wiki/xacro">

  <!-- Cameras front (coincide with front hdr cam) -->
  <link name="cameras_front" />
  <joint name="box_base_to_cameras_front" type="fixed">   
    <origin xyz=" ${calib['box_base_to_cameras_front']['x']}
                  ${calib['box_base_to_cameras_front']['y']}
                  ${calib['box_base_to_cameras_front']['z']}" 
            rpy=" ${calib['box_base_to_cameras_front']['roll']}
                  ${calib['box_base_to_cameras_front']['pitch']}
                  ${calib['box_base_to_cameras_front']['yaw']}" />
    <parent link="box_base" />
    <child link="cameras_front" />
  </joint>

  <!-- HDR camera front -->
  <link name="hdr_front" />
  <joint name="cameras_front_to_hdr_front" type="fixed">   
    <origin xyz=" ${calib['cameras_front_to_hdr_front']['x']}
                  ${calib['cameras_front_to_hdr_front']['y']}
                  ${calib['cameras_front_to_hdr_front']['z']}" 
            rpy=" ${calib['cameras_front_to_hdr_front']['roll']}
                  ${calib['cameras_front_to_hdr_front']['pitch']}
                  ${calib['cameras_front_to_hdr_front']['yaw']}" />
    <parent link="cameras_front" />
    <child link="hdr_front" />
  </joint>

  <!-- Alphasense front left -->
  <link name="alphasense_front_left" />
  <joint name="hdr_front_to_alphasense_front_left" type="fixed">   
    <origin xyz=" ${calib['hdr_front_to_alphasense_front_left']['x']}
                  ${calib['hdr_front_to_alphasense_front_left']['y']}
                  ${calib['hdr_front_to_alphasense_front_left']['z']}" 
            rpy=" ${calib['hdr_front_to_alphasense_front_left']['roll']}
                  ${calib['hdr_front_to_alphasense_front_left']['pitch']}
                  ${calib['hdr_front_to_alphasense_front_left']['yaw']}" />
    <parent link="hdr_front" />
    <child link="alphasense_front_left" />
  </joint>

  <!-- Alphasense front middle -->
  <link name="alphasense_front_middle" />
  <joint name="alphasense_front_left_to_alphasense_front_middle" type="fixed">   
    <origin xyz=" ${calib['alphasense_front_left_to_alphasense_front_middle']['x']}
                  ${calib['alphasense_front_left_to_alphasense_front_middle']['y']}
                  ${calib['alphasense_front_left_to_alphasense_front_middle']['z']}" 
            rpy=" ${calib['alphasense_front_left_to_alphasense_front_middle']['roll']}
                  ${calib['alphasense_front_left_to_alphasense_front_middle']['pitch']}
                  ${calib['alphasense_front_left_to_alphasense_front_middle']['yaw']}" />
    <parent link="alphasense_front_left" />
    <child link="alphasense_front_middle" />
  </joint>

  <!-- Alphasense front right -->
  <link name="alphasense_front_right" />
  <joint name="alphasense_front_middle_to_alphasense_front_right" type="fixed">   
    <origin xyz=" ${calib['alphasense_front_middle_to_alphasense_front_right']['x']}
                  ${calib['alphasense_front_middle_to_alphasense_front_right']['y']}
                  ${calib['alphasense_front_middle_to_alphasense_front_right']['z']}" 
            rpy=" ${calib['alphasense_front_middle_to_alphasense_front_right']['roll']}
                  ${calib['alphasense_front_middle_to_alphasense_front_right']['pitch']}
                  ${calib['alphasense_front_middle_to_alphasense_front_right']['yaw']}" />
    <parent link="alphasense_front_middle" />
    <child link="alphasense_front_right" />
  </joint>

  <!-- Zed right -->
  <link name="zed_right" />
  <joint name="alphasense_front_right_to_zed_right" type="fixed">   
    <origin xyz=" ${calib['alphasense_front_right_to_zed_right']['x']}
