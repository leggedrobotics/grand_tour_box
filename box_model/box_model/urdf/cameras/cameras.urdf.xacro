  <link name="alphasense_right" />
  <joint name="hdr_right_to_alphasense_right" type="fixed">   
    <origin xyz=" ${calib['hdr_right_to_alphasense_right']['x']}
                  ${calib['hdr_right_to_alphasense_right']['y']}
                  ${calib['hdr_right_to_alphasense_right']['z']}" 
            rpy=" ${calib['hdr_right_to_alphasense_right']['roll']}
                  ${calib['hdr_right_to_alphasense_right']['pitch']}
                  ${calib['hdr_right_to_alphasense_right']['yaw']}" />
    <parent link="hdr_right" />
    <child link="alphasense_right" />
  </joint>

  <!-- Cameras left (coincide with left hdr cam) -->
  <link name="cameras_left" />
  <joint name="box_base_to_cameras_left" type="fixed">   
    <origin xyz=" ${calib['box_base_to_cameras_left']['x']}
