                  ${calib['alphasense_front_right_to_zed_right']['z']}" 
            rpy=" ${calib['alphasense_front_right_to_zed_right']['roll']}
                  ${calib['alphasense_front_right_to_zed_right']['pitch']}
                  ${calib['alphasense_front_right_to_zed_right']['yaw']}" />
    <parent link="alphasense_front_right" />
    <child link="zed_right" />
  </joint>

  <!-- Zed left -->
  <link name="zed_left" />
  <joint name="zed_right_to_zed_left" type="fixed">   
    <origin xyz=" ${calib['zed_right_to_zed_left']['x']}
                  ${calib['zed_right_to_zed_left']['y']}
                  ${calib['zed_right_to_zed_left']['z']}" 
            rpy=" ${calib['zed_right_to_zed_left']['roll']}
                  ${calib['zed_right_to_zed_left']['pitch']}
                  ${calib['zed_right_to_zed_left']['yaw']}" />
    <parent link="zed_right" />
    <child link="zed_left" />
  </joint>

  <!-- Cameras right (coincide with right hdr cam) -->
