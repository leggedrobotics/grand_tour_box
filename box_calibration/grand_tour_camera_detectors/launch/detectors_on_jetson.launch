<launch>

    <node name="zed_left_detector" pkg="grand_tour_camera_detectors" type="grand_tour_camera_detector"
          output="screen">
        <param name="image_topic" value="/gt_box/zed2i_driver_node/left_raw/image_raw_color"/>
        <param name="show_extraction_video" value="false"/>
        <param name="grid_size_x" value="6"/>
        <param name="grid_size_y" value="6"/>
        <param name="tag_size" value="0.083"/>
        <param name="tag_spacing" value="0.3"/>
    </node>

    <node name="zed_right_detector" pkg="grand_tour_camera_detectors" type="grand_tour_camera_detector"
          output="screen">
        <param name="image_topic" value="/gt_box/zed2i_driver_node/right_raw/image_raw_color"/>
        <param name="show_extraction_video" value="false"/>
        <param name="grid_size_x" value="6"/>
        <param name="grid_size_y" value="6"/>
        <param name="tag_size" value="0.083"/>
        <param name="tag_spacing" value="0.3"/>
    </node>

    <node name="hdr_left_detector" pkg="grand_tour_camera_detectors" type="grand_tour_camera_detector"
          output="screen">
        <param name="image_topic" value="/gt_box/hdr_left/image_raw"/>
        <param name="show_extraction_video" value="false"/>
        <param name="grid_size_x" value="6"/>
        <param name="grid_size_y" value="6"/>
        <param name="tag_size" value="0.083"/>
        <param name="tag_spacing" value="0.3"/>
    </node>

    <node name="hdr_center_detector" pkg="grand_tour_camera_detectors" type="grand_tour_camera_detector"
          output="screen">
        <param name="image_topic" value="/gt_box/hdr_front/image_raw"/>
        <param name="show_extraction_video" value="false"/>
        <param name="grid_size_x" value="6"/>
        <param name="grid_size_y" value="6"/>
        <param name="tag_size" value="0.083"/>
        <param name="tag_spacing" value="0.3"/>
    </node>

    <node name="hdr_right_detector" pkg="grand_tour_camera_detectors" type="grand_tour_camera_detector"
          output="screen">
        <param name="image_topic" value="/gt_box/hdr_right/image_raw"/>
        <param name="show_extraction_video" value="false"/>
        <param name="grid_size_x" value="6"/>
        <param name="grid_size_y" value="6"/>
        <param name="tag_size" value="0.083"/>
        <param name="tag_spacing" value="0.3"/>
    </node>


</launch>
