<?xml version="1.0"?>
<package format="2">
  <name>zed2i_recording_driver_ros</name>
  <version>0.0.0</version>
  <description>The zed2i_recording_driver_ros package</description>
  <maintainer email="kpatterson@student.ethz.ch">Kappi Patterson</maintainer>
  <license>MIT</license>
  <buildtool_depend>catkin</buildtool_depend>

  <build_depend>roscpp</build_depend>
  <build_depend>std_msgs</build_depend>
  <build_depend>message_generation</build_depend>

  <exec_depend>roscpp</exec_depend>
  <exec_depend>std_msgs</exec_depend>
  <exec_depend>message_runtime</exec_depend>

  <!-- External dependencies not managed by ROS -->
  <!-- These dependencies need to be installed manually -->
  <depend>zed</depend>
  <depend>cuda</depend>

  <export>
  </export>
</package>