<?xml version="1.0" encoding="utf-8"?>

<launch>
  <!-- Path to the rqt perspective file -->
<arg name="rqt_perspective_path"                    default="$(find rqt_micro_plugin)/config/recording.perspective"/>

  <!-- Launch rqt -->
<node name="rqt_gui" pkg="rqt_gui" type="rqt_gui" output="screen" respawn="false"/>

</launch>
