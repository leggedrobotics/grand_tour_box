<launch>
  <arg name="box_namespace" default="gt_box"/>
  <arg name="calibration" default="no"/>
  <group ns="$(arg box_namespace)">
    <node pkg="box_health" type="merge_health_status.py" name="box_health_merger" output="screen">
      <param name="capability_group" value="Monitoring"/>
    </node>
  </group>
</launch>